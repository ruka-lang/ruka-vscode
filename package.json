{
  "name": "ruka-vscode",
  "displayName": "Warp Programming Language",
  "description": "Warp Programming Language",
  "version": "0.0.1",
  "homepage": "https://www.ruka-lang.org",
  "respository": {
     "type": "git",
     "url": "https://github.com/ruka-lang/ruka-vscode"
  },
  "icon": "assets/ruka-main-transparent-icon.png",
  "license": ["MIT", "APACHE"],
  "publisher": "ruka-lang",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ruka",
      "aliases": ["Ruka", "ruka"],
      "extensions": [".ruka", ".rk"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ruka",
      "scopeName": "source.ruka",
      "path": "./syntaxes/ruka.tmLanguage.json"
    }]
  }
}
