{
  "name": "warp-vscode",
  "displayName": "Warp Programming Language",
  "description": "Warp Programming Language",
  "version": "0.0.1",
  "homepage": "https://www.warp-lang.org",
  "respository": {
     "type": "git",
     "url": "https://github.com/warp-lang/warp-vscode"
  },
  "icon": "assets/warp-main-transparent-icon.png",
  "license": ["MIT", "APACHE"],
  "publisher": "warp-lang",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "warp",
      "aliases": ["Warp", "warp"],
      "extensions": [".warp", ".wr"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "warp",
      "scopeName": "source.warp",
      "path": "./syntaxes/warp.tmLanguage.json"
    }]
  }
}
