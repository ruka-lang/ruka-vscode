{
  "name": "amulet-vsc",
  "displayName": "Amulet Programming Language",
  "description": "Amulet Programming Language",
  "version": "0.0.1",
  "homepage": "https://www.amulet-lang.org",
  "respository": {
     "type": "git",
     "url": "https://github.com/amulet-lang/amulet-vsc"
  },
  "icon": "assets/amulet-main-transparent-icon.png",
  "license": ["MIT", "APACHE"],
  "publisher": "amulet-lang",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "amulet",
      "aliases": ["Amulet", "amulet"],
      "extensions": [".amulet", ".amu"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "amulet",
      "scopeName": "source.amulet",
      "path": "./syntaxes/amulet.tmLanguage.json"
    }]
  }
